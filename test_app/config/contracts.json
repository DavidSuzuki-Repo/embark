{
  "default": {
    "versions": {
      "web3.js": "0.19.1",
      "solc": "0.4.11"
    },
    "deployment": {
      "host": "localhost",
      "port": 8545,
      "type": "rpc"
    },
    "dappConnection": [
      "$WEB3",
      "localhost:8545"
    ],
    "gas": "auto",
    "contracts": {
      "SimpleStorage": {
        "args": [
          100
        ]
      },
      "AnotherStorage": {
        "args": [
          "$SimpleStorage"
        ]
      },
      "Token": {
        "deploy": false,
        "args": [1000]
      },
      "MyToken": {
        "instanceOf": "Token"
      },
      "MyToken2": {
        "instanceOf": "Token",
        "args": [200]
      },
      "AlreadyDeployedToken": {
        "address": "0x123",
        "instanceOf": "Token"
      }
    }
  },
  "development": {
    "contracts": {
      "MyToken2": {
        "instanceOf": "Token",
        "args": [2000]
      }
    }
  }
}
